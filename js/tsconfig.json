{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2015",
    "types": [],
    "lib": ["es5", "es2015"],
    "noEmit": true,
    "noUncheckedIndexedAccess": true,
    "strict": true,
    "jsx": "react",
    "isolatedModules": true,
    "noUncheckedSideEffectImports": true,
    "moduleDetection": "force",
    "skipLibCheck": true,
    "esModuleInterop": true
  }
}
